    "polymer-redux": "^1.0.3",
    "redux": "^3.7.2",
    "redux-router5": "^5.0.0",
    "router5": "^5.1.7",
    "router5.helpers": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "concurrently": "^3.5.0",
    "http-server": "^0.10.0",
